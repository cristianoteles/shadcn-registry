{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "devemp",
  "homepage": "https://devemp.com.br",
  "items": [
    {
      "name": "feature-configs",
      "type": "registry:item",
      "title": "Prepare configs",
      "description": "Create structure for configs",
      "files": [
        {
          "path": "registry/features/configs/site.ts",
          "type": "registry:file",
          "target": "features/devemp/configs/site.tsx"
